{
  "name": "marwaadiinteriorwalaa-shopify-theme",
  "version": "1.0.0",
  "description": "Shopify theme based on Horizon with Online Store 3.0",
  "main": "index.js",
  "scripts": {
    "dev": "shopify theme dev",
    "dev:open": "shopify theme dev --open",
    "build": "shopify theme check",
    "deploy:dev": "shopify theme push --development",
    "deploy:staging": "shopify theme push --theme=$SHOPIFY_STAGING_THEME_ID",
    "deploy:prod": "shopify theme push --live",
    "pull": "shopify theme pull",
    "check": "shopify theme check",
    "lint": "shopify theme check",
    "format": "prettier --write \"**/*.{liquid,js,css,scss,json}\"",
    "test": "npm run check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ankita-mundada/github-demo.git"
  },
  "keywords": [
    "shopify",
    "theme",
    "ecommerce",
    "online-store-3.0",
    "horizon"
  ],
  "author": "Ankita Mundada",
  "license": "MIT",
  "devDependencies": {
    "@shopify/cli": "^3.87.4",
    "@shopify/theme": "^3.87.4",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}